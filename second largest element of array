class Solution {
    
    public int getSecondLargest(int[] arr) {
        
        int max=0;
        int second_max=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i]>max){
               
                second_max=max;
                 max=arr[i];
            }
            else if(arr[i]!=max && arr[i]>second_max){
                second_max=arr[i];
            }
        
        }
        return second_max==0?-1:second_max;
    }
}
