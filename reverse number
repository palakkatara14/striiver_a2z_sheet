class Solution {
    public int reverse(int x) {
        long sum=0;
        int r;
        while(x!=0){
           r=x%10;
           sum=sum*10+r;
           x=x/10;
           
       
        
            if(sum>Integer.MAX_VALUE ||sum <Integer.MIN_VALUE){
                return 0;
            }
        }
        return (int)sum;

        
    }
    
    
}
